<!DOCTYPE html>


    <!--Error-->
    <div id="error_message" class="ui floating negative message" style="display: none">
        <div class="header"></div>
        <p class="message"></p>
    </div>
    <!--Page Header-->
    <div class="ui tall stacked segment">
        <h4 class="ui header">
            <i class="yellow color tag icon"></i>
            <div class="content">
                Annotator.js Example
                <div class="sub header">
                    Annotate/mark a certain fragment of text with different tags and displays the corresponding note cards in the sidebar.
                </div>
            </div>
        </h4>
    </div>
    <!--Body-->
    <div class="ui grid container">
        <div class="two column row">
            <!--Saved Annotations-->
            <div class="six wide column">
                <div class="ui fluid card">
                    <div class="content">
                        <!--Header-->
                        <h5 class="ui header">
                            <i class="green color tags icon"></i>
                            <div class="content">
                                Saved Annotations
                                <div class="meta">
                                    <span>Try deleting a saved annotation</span>
                                </div>
                            </div>
                        </h5>
                        <!--Annotations List-->
                        <div id="annotations_list">

                            <div class="ui raised segment">
                                <a class="ui blue ribbon label">Internal</a>

                                <p class="p-around_small">euismod, ac facilisis </p>

                                <div class="fluid ui button" onclick="App.handlers.deleteAnnotation( 'annotation_1' )">Delete</div>
                            </div>

                            <div class="ui raised segment">
                                <a class="ui orange ribbon label">Requirement</a>

                                <p class="p-around_small">llicitudin non. P</p>

                                <div class="fluid ui button" onclick="App.handlers.deleteAnnotation( 'annotation_2' )">Delete</div>
                            </div>

                            <div class="ui raised segment">
                                <a class="ui orange ribbon label">Requirement</a>

                                <p class="p-around_small">per enim euismod,</p>

                                <div class="fluid ui button" onclick="App.handlers.deleteAnnotation( 'annotation_3' )">Delete</div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <!--Transcript-->
            <div class="ten wide column">
                <div class="ui fluid card">
                    <div class="content">
                        <!--Header-->
                        <h5 class="ui header">
                            <i class="violet color phone square icon"></i>
                            <div class="content">
                                Transcript
                                <div class="meta">
                                    <span>Try selecting a block of text</span>
                                </div>
                            </div>
                        </h5>
                        <!--Feed-->
                        <div class="ui feed">
                            <div class="event">
                                <div class="label">
                                    <i class="olive color user icon"></i>
                                </div>
                                <div class="content">
                                    <div class="summary">
                                        <a>Joe Henderson</a> spoke at
                                        <div class="date">
                                            02:03
                                        </div>
                                    </div>
                                    <div class="example text" spellcheck="false" contenteditable="true">
                                        Lorem ipsum dolor sit amet, consectetur adipiscing elit. In ultricies est ullamcorper enim <annotation id="annotation_1" notes="euismod, ac facilisis " type="Internal">euismod, ac facilisis </annotation>tortor ultricies. Quisque interdum enim tortor, hendrerit efficitur nunc sollicitudin non. Phasellus diam ligula, consequat sollicitudin quam vel, laoreet ultrices quam. Etiam varius lectus sagittis placerat finibus.
                                    </div>
                                </div>
                            </div>
                            <div class="event">
                                <div class="label">
                                    <i class="olive color user icon"></i>
                                </div>
                                <div class="content">
                                    <div class="summary">
                                        <a>Jane Doe</a> spoke at
                                        <div class="date">
                                            05:13
                                        </div>
                                    </div>
                                    <div class="example text" spellcheck="false" contenteditable="true">
                                        Lorem ipsum dolor sit amet, consectetur adipiscing elit. In ultricies est ullamcor<annotation id="annotation_3" notes="per enim euismod," type="Requirement">per enim euismod,</annotation> ac facilisis tortor ultricies. Quisque interdum enim tortor, hendrerit efficitur nunc so<annotation id="annotation_2" notes="llicitudin non. P" type="Requirement">llicitudin non. P</annotation>hasellus diam ligula, consequat sollicitudin quam vel, laoreet ultrices quam. Etiam varius lectus sagittis placerat finibus.
                                    </div>
                                </div>
                            </div>
                            <div class="event">
                                <div class="label">
                                    <i class="olive color user icon"></i>
                                </div>
                                <div class="content">
                                    <div class="summary">
                                        <a>John Doe</a> spoke at
                                        <div class="date">
                                            08:40
                                        </div>
                                    </div>
                                    <div class="example text" spellcheck="false" contenteditable="true">
                                        Lorem ipsum dolor sit amet, consectetur adipiscing elit. In ultricies est ullamcorper enim euismod, ac facilisis tortor ultricies. Quisque interdum enim tortor, hendrerit efficitur nunc sollicitudin non. Phasellus diam ligula, consequat sollicitudin quam vel, laoreet ultrices quam. Etiam varius lectus sagittis placerat finibus.
                                    </div>
                                </div>
                            </div>
                            <div class="event">
                                <div class="label">
                                    <i class="olive color user icon"></i>
                                </div>
                                <div class="content">
                                    <div class="summary">
                                        <a>Mike Wright</a> spoke at
                                        <div class="date">
                                            15:00
                                        </div>
                                    </div>
                                    <div class="example text" spellcheck="false" contenteditable="true">
                                        Lorem ipsum dolor sit amet, consectetur adipiscing elit. In ultricies est ullamcorper enim euismod, ac facilisis tortor ultricies. Quisque interdum enim tortor, hendrerit efficitur nunc sollicitudin non. Phasellus diam ligula, consequat sollicitudin quam vel, laoreet ultrices quam. Etiam varius lectus sagittis placerat finibus.
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--Annotator Popover Contents-->
    <div id="annotate_settings" class="ui form raised segments" style="margin: 0px; display: none;">
        <!--Notes-->
        <div class="required field p-around_small">
            <label>Type some notes</label>
            <textarea rows="5" onchange="App.handlers.captureNotes( this )"></textarea>
        </div>
        <!--Tags-->
        <div class="p-around_small m-bottom_medium">
            <div class="ui dropdown labeled icon fluid button upward" tabindex="0">
                <i class="tag icon"></i>
                <span class="text">Select Tag</span>
                <div class="menu transition hidden" tabindex="-1">
                    <div class="ui icon search input">
                        <i class="search icon"></i>
                        <input type="text" placeholder="Search tags...">
                    </div>
                    <div class="divider"></div>
                    <div class="header">
                        <i class="tags icon"></i>
                        Tag Label
                    </div>
                    <div class="scrolling menu">
                        <div name="Requirement" class="item">
                            <div class="ui orange empty circular label"></div>
                            Requirement
                        </div>
                        <div name="Backlog" class="item">
                            <div class="ui teal empty circular label"></div>
                            Backlog
                        </div>
                        <div name="Internal" class="item">
                            <div class="ui blue empty circular label"></div>
                            Internal
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--Action Buttons-->
        <div class="field p-around_small m-bottom_medium">
            <button class="ui right floated olive button" onclick="App.handlers.saveAnnotation();">Save</button>
            <button class="ui left floated button" onclick="App.handlers.cancelAnnotation();">Cancel</button>
        </div>
        <!--Placeholder-->
        <div class="field p-around_small m-bottom_medium">
        </div>
        <!--Arrow-->
        <div id="arrow" data-popper-arrow="" style=""></div>
    </div>
    <!--Footer-->
    <h4 class="ui horizontal divider header">
        Built with <span class="love">♥</span> by Deepak
    </h4>
    <!--Backdrop-->
    <div class="ui dimmer page active backdrop" style="display: none;"></div>
    <!--Template-->







<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<title>Annotator.js Example</title>
<!--Favicon-->
<link rel="icon" type="image/png" href="img/favicon.png">
<!--Semantic UI CSS-->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.4.1/semantic.min.css">
<!--Annotator CSS-->
<link rel="stylesheet" href="~/css/annotator.css">
<!--Custom Styles-->
<link rel="stylesheet" href="~/css/styles.css">

@section Scripts
{
    <script type="text/javascript" async="" src="https://ssl.google-analytics.com/ga.js"></script>
    <script id="annotations_tmpl" type="text/x-jsrender" data-jsv-tmpl="jsvTmpl">
        {{for annotations}}
        <div class="ui raised segment">
            <a class="ui {{:color}} ribbon label">{{:type}}</a>

            <p class="p-around_small">{{:notes}}</p>

            <div class="fluid ui button" onclick="App.handlers.deleteAnnotation( '{{:id}}' )">Delete</div>
        </div>
        {{/for}}
    </script>
    <!--Scripts-->
    <!--Required for Annotator.js-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <!--Required for Annotator.js-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.4.2/umd/popper.min.js"></script>
    <!--Optional (used for Templating)-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jsrender/1.0.6/jsrender.min.js"></script>
    <!--Optional (used for Semantic UI)-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.4.1/semantic.min.js"></script>
    <!--Annotator jQuery Plugin-->
    <script type="text/javascript" src="~/js/annotator.js"></script>
